// normative scenario (limited traffic zone)
// example with obligation to use portals

// run with
//     ./gradlew :examples:run --args="grid-norms1.mas2j"


MAS grid4 {

    environment: example.normative_grid.LTZGridJasonEnv(init(13,1),goal(20,1),scenario("LTZ"),portals) // goal(13,21) goal(20,1)

    agents:
        car_ag grid_recovery.asl
             agentClass jason.future.ForeseeProblemAgent("src/org/norms1.npl");

    aslSourcePath: "src/agt";
}
